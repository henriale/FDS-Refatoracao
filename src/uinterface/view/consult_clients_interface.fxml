<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.*?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
          prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8"
          fx:controller="uinterface.controller.ConsultClientsController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="40.0" minHeight="10.0" prefHeight="40.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <ListView fx:id="clients" prefHeight="200.0" prefWidth="200.0" GridPane.columnSpan="3" GridPane.rowSpan="2">
            <GridPane.margin>
                <Insets left="10.0" right="10.0" top="10.0"/>
            </GridPane.margin>
        </ListView>
        <Button fx:id="backBtn" cancelButton="true" defaultButton="true" mnemonicParsing="false" onAction="#moveMain"
                text="Voltar" GridPane.columnIndex="2" GridPane.halignment="RIGHT" GridPane.rowIndex="2"
                GridPane.valignment="CENTER">
            <GridPane.margin>
                <Insets right="10.0"/>
            </GridPane.margin>
        </Button>
    </children>
</GridPane>
